# Server Configuration
NODE_ENV=development
PORT=3000

# Redis
REDIS_URL="redis://localhost:6379"

# Cache Configuration
CACHE_TTL_SECONDS=86400  # 24 horas (24 * 60 * 60)
CACHE_STALE_TTL_SECONDS=172800  # 48 horas para fallback (2 * 24 * 60 * 60)

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=60

# Logging
LOG_LEVEL=debug

# YouTube API
YOUTUBE_API_KEY=

# AliExpress Affiliate API
ALIEXPRESS_APP_KEY=
ALIEXPRESS_APP_SECRET=
ALIEXPRESS_TRACKING_ID=
CATEGORY_RESOLUTION_MODE=api

# Google Trends Configuration - BALANCED (más rápido pero con anti-bloqueo)
GOOGLE_TRENDS_MAX_RETRIES=5
GOOGLE_TRENDS_RETRY_DELAY_MS=10000  # 10 segundos para reintentos
GOOGLE_TRENDS_REQUEST_DELAY_MS=5000  # 5 segundos entre requests
GOOGLE_TRENDS_TIMEOUT_MS=90000  # 90 segundos timeout
GOOGLE_TRENDS_CONCURRENCY=1
